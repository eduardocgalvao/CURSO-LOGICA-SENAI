programa {
  funcao inicio() {
    inteiro num, num2, num3

    escreva ("DIGITE O 1° NÚMERO: ")
    leia (num)

    escreva ("DIGITE O 2° NÚMERO: ")
    leia (num2)

    escreva ("DIGITE O 3° NÚMERO: ")
    leia (num3)

    real result = (num + num2 + num3) / 3

    escreva ("A MEDIA ARITMETICA DESSES NÚMEROS É ", result)
  }
}
