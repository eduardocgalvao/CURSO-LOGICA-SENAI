programa {
  funcao inicio() {
    inteiro num

    
    escreva ("REGRA: O NUMÉRO PODE SER TEM QUE SER MENOR QUE 5 OU MAIOR QUE 15 \n")
   
    escreva ("Digite um número: ")
    leia (num)

      

    se (num < 5 ou num > 15){
      
      
      escreva ("O NUMERO SE ENCAIXA NAS REGRAS \n")

    }senao{

      escreva ("O NUMERO NÃO SE ENCAIXA NAS REGRAS \n")  

    }
  }
}
